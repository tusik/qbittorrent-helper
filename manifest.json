{
    "name": "qbittorrent-helper",
    "version": "0.0.1",
    "description": "Chrome extension to help with qBittorrent",
    "manifest_version": 3,
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "icons": {
        "16": "icon/logo-16.png",
        "48": "icon/logo-48.png",
        "128": "icon/logo-128.png"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "webRequest",
        "cookies"
    ],
    "action": {
        "default_popup": "html/settings.html"
    },
    "host_permissions": ["*://*/*"]
}
